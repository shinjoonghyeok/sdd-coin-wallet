<% include ./../_website_theme/header.ejs %>
<div class="page-wrapper">
	<% include ./../_website_theme/nav.ejs %>
	<!-- Banner -->
	<div class="banner static-banner clearfix ">
		<div class="slider-wrapper theme-default">
			<div id="sliderNivo" class="nivoSlider">
				<img src="/images/racing_banner.jpg" />
				<img src="/images/racing_banner2.jpg" />
				<img src="/images/racing_banner3.jpg" />
			</div>
		</div>
		<!-- <img src="/images/racing_banner.jpg" alt=""> -->
	</div>
	<!-- section start -->
	<!-- ================ -->
	<section id="sdd_coin" class="pv-45 clearfix mb-5 ">
		<div class="container mb-5">
			<div class="row justify-content-lg-center mb-5">
				<div class="col-lg-10">
					<h1 class="intro text-center mt-4 "><strong><%= __("index_1_Main") %></strong></h1>
				</div>
			</div>
			<div class="row mt-5">
				<div class="col-lg-6">
					<div class="feature-box-2 object-non-visible animated object-visible fadeInLeftSmall" data-animation-effect="fadeInDownSmall" data-effect-delay="100">
						<span class="icon without-bg"><i class="fa text-default fa-laptop"></i></span>
						<div class="body">
							<h3 class="title"><%= __("index_1_Title1") %></h3>
							<p class="pitch_text"><%= __("index_1_Context1") %></p>
						</div>
					</div>
				</div>
				<div class="col-lg-6">
					<div class="feature-box-2 object-non-visible animated object-visible fadeInRightSmall" data-animation-effect="fadeInDownSmall" data-effect-delay="100">
						<span class="icon without-bg"><i class="fa text-default fa-heart-o"></i></span>
						<div class="body">
							<h3 class="title"><%= __("index_1_Title2") %></h3>
							<p class="pitch_text"><%= __("index_1_Context2") %></p>
						</div>
					</div>
				</div>
			</div>
			<div class="row justify-content-md-center">
				<div class="col-lg-12">
					<div class="feature-box-2 object-non-visible animated object-visible fadeInUpSmall" data-animation-effect="fadeInDownSmall" data-effect-delay="100">
						<span class="icon without-bg"><i class="fa text-default fa-money"></i></span>
						<div class="body">
							<h3 class="title"><%= __("index_1_Title3") %></h3>
							<p class="pitch_text"><%= __("index_1_Context3") %></p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- section end -->
