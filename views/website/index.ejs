		<div class="container pv-45">
			<div class="row justify-content-lg-center mb-5">
				<div class="col-lg-10">
					<h1 class="intro text-center mt-4 "><strong><%= __("index_2_Main") %></strong></h1>
				</div>
			</div>
			<div class="row mt-5">
				<!-- Trust -->
				<div class="col-lg-4">
