				</div>
			</div>
			<div class="row justify-content-lg-center">
				<div class="col-lg-12 " data-animation-effect="fadeInDownSmall">
					<div class="pv-30 ph-20 feature-box sdd-box bordered shadow text-center object-non-visible" data-animation-effect="fadeInDownSmall" data-effect-delay="100">
						<img src="/images/<%= roadmapIMG %>" class="img-fluid">
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- section end -->
	<!-- section start -->
	<!-- ================ -->
	<section id="collaborate" class=" pv-45 light-gray-bg clearfix ">
		<div class="container pv-45">
			<div class="row justify-content-lg-center mb-5">
				<div class="col-lg-10">
					<h1 class="intro text-center mt-4 "><strong><%= __("index_3_Title") %></strong></h1>
					<p class="pitch_text" data-animation-effect="fadeInUpSmall"><%= __("index_3_Context") %></p>
				</div>
			</div>
			<div class="row ">
				<div class="col-md-4 text-center" data-animation-effect="fadeInDownSmall">
					<img src="/images/g2e.png" class="img-fluid">
				</div>
				<div class="col-md-4 text-center" data-animation-effect="fadeInDownSmall">
					<img src="/images/gli.png" class="img-fluid">
				</div>
				<!--
				<div class="col-md-3 text-center" data-animation-effect="fadeInDownSmall">
					<img src="/images/melonbit.png" class="img-fluid">
				</div>
				-->
				<div class="col-md-4 text-center" data-animation-effect="fadeInDownSmall">
					<img src="/images/gicc.png" class="img-fluid">
				</div>
			</div>
		</div>
	</section>
	<!-- section end -->
</div>

<% include ./../_website_theme/footer.ejs %>

<script>
	$(document).ready(function () {
		// Nivo
		$('#sliderNivo').nivoSlider({
			effect: 'boxRainGrow,boxRainGrowReverse,fold,boxRain,sliceUp',
			slices: 15,
			boxCols: 8,
			boxRows: 4,
			animSpeed: 2000,
			pauseTime: 3000
		});
	});
</script>